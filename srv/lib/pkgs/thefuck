#-------------------------------------------------------------------
# CHECK
#-------------------------------------------------------------------
thefuck::check() { pip show thefuck 2> /dev/null 1>&2; return $?; }
#-------------------------------------------------------------------
# INSTALL
#-------------------------------------------------------------------
thefuck::install()
{
	pip install thefuck --upgrade --quiet
}
#-------------------------------------------------------------------
# REMOVE
#-------------------------------------------------------------------
thefuck::remove()
{
	pip remove thefuck --quiet
}
